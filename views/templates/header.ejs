<script>
  function customload(e) {
    event.preventDefault();
    if (
      typeof sessionStorage.token !== "undefined" &&
      sessionStorage.token.length > 0
    ) {
      location.href = e.href + "?token=" + sessionStorage.token;
    } else {
      location.href = e.href;
    }
  }
</script>
